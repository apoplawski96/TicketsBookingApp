<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             xmlns:tools="http://schemas.android.com/tools"
             android:id="@+id/cell_content_view"
             android:layout_width="match_parent"
             android:layout_height="wrap_content">


    <android.support.constraint.ConstraintLayout android:layout_width="match_parent"
                                                 android:layout_height="330dp"
                                                 android:background="@color/whiteColor"
                                                 android:layout_marginHorizontal="12dp"
                                                 android:layout_marginVertical="1dp">


        <TextView
                android:text="From"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView12" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.08"
                app:layout_constraintVertical_bias="0.01999998" android:textSize="11sp"/>

        <TextView
                android:text="To"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView13" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.72"
                app:layout_constraintVertical_bias="0.01999998" android:textSize="11sp"/>
        <TextView
                android:text="VIE"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/fromShort2" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.08" android:textSize="32sp"
                android:textColor="@android:color/black" app:layout_constraintVertical_bias="0.060000002"/>

        <TextView
                android:text="MAD"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/toShort2" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.84" android:textSize="32sp"
                android:textColor="@android:color/black" app:layout_constraintVertical_bias="0.06999999"/>
        <ImageView
                android:layout_width="51dp"
                android:layout_height="44dp" app:srcCompat="@drawable/ic_arrow_forward"
                android:id="@+id/imageView4" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
                android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                app:layout_constraintVertical_bias="0.060000002"/>

        <TextView
                android:text="Madrid, Spain"
                android:textColor="@color/colorPrimaryLight"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/cityAndCountryDestination2" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.85"
                android:textSize="12sp" app:layout_constraintVertical_bias="0.19999999"/>

        <TextView
                android:text="Vienna, Austria"
                android:textColor="@color/colorPrimaryLight"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/cityAndCountryFrom2" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.1"
                android:textSize="12sp" app:layout_constraintVertical_bias="0.19999999"/>

        <TextView
                android:text="Nov 1, 2019"
                android:textColor="#111111"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/departureDate2" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.1"
                android:textSize="14sp" app:layout_constraintVertical_bias="0.31"/>

        <TextView
                android:text="Nov 2, 2019"
                android:textColor="#111111"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/arrivalDate2" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.86"
                android:textSize="14sp" app:layout_constraintVertical_bias="0.31"/>

        <TextView
                android:text="4:30 PM"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/departureTime2" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.09"
                android:textSize="20sp" app:layout_constraintVertical_bias="0.41000003"/>

        <TextView
                android:text="02:50 AM"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/arrivalTime2" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.89"
                android:textSize="20sp" app:layout_constraintVertical_bias="0.41000003"/>
        <FrameLayout
                android:layout_width="371dp"
                android:layout_height="161dp" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintVertical_bias="1.0">

            <android.support.constraint.ConstraintLayout android:layout_width="match_parent"
                                                         android:layout_height="match_parent"></android.support.constraint.ConstraintLayout>

        </FrameLayout>

    </android.support.constraint.ConstraintLayout>

</FrameLayout>